<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane prefHeight="470.0" prefWidth="747.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.uji.ei1048.meteorologia.view.RootLayout">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <Menu mnemonicParsing="false" text="Menu">
                <MenuItem mnemonicParsing="false" text="Close" />
            </Menu>
            <Menu fx:id="apiMenu" mnemonicParsing="false" text="API">
                <MenuItem fx:id="ow" mnemonicParsing="false" onAction="#setOwApi" text="OpenWeather" />
                <MenuItem fx:id="aw" mnemonicParsing="false" onAction="#setAwApi" text="AccuWeather" />
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
                <MenuItem mnemonicParsing="false" text="About" />
            </Menu>
        </MenuBar>
    </top>
    <center>
        <SplitPane fx:id="sp" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
    </center>
</BorderPane>
